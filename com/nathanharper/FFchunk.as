/*
the actual "chunks" of force field.
*/
package com.nathanharper{
	import flash.display.MovieClip;
	import flash.geom.Point;
	public class FFchunk extends StaticItem{
		private var myPoint:Point;
		public function FFchunk(num:Number,p:Point){
			this.rotation=num;
			myPoint=p;
		}
		public function getPoint(){
			return myPoint;
		}
	}
}